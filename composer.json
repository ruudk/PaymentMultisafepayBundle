        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "symfony/framework-bundle": "~2.8|^3",
        "symfony/form": "~2.8|^3",
        "jms/payment-core-bundle": "~1.0",
        "omnipay/multisafepay": "~2.0",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": { "Ruudk\\Payment\\MultisafepayBundle\\": "src" }
